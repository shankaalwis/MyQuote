<div id="<%= dom_id quote %>">
  <p>
    <strong>Quote text:</strong>
    <%= quote.quote_text %>
  </p>

  <p>
    <strong>Publication year:</strong>
    <%= quote.publication_year %>
  </p>

  <p>
    <strong>Owner comment:</strong>
    <%= quote.owner_comment %>
  </p>

  <p>
    <strong>Is public:</strong>
    <%= quote.is_public %>
  </p>

  <p>
    <strong>User:</strong>
    <%= quote.user_id %>
  </p>

  <p>
    <strong>Philosopher:</strong>
    <%= quote.philosopher_id %>
  </p>

</div>
